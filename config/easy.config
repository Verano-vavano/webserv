http { listen 8080;
server{
	root uber-frosch;
	error_page 404 /404.html;
	location /cgi-bin/ {
		root uber-frosch;
		cgi_exec .cgi;
		cgi_exec *.py;
		cgi_interpreter .py /bin/python3;
	}
	location / { root uber-frosch; index index.html; }
}server 9000 { location /bin {alias uber-frosch;cgi_exec cgi; cgi_interpreter .pl /usr/bin/perl;}}
 }
